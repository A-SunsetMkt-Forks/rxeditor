{
  "name": "@rxdrag/react-fieldy",
  "version": "1.0.0",
  "description": "Core",
  "author": "Water.li",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/resize-observer-browser": "^0.1.7",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "jest": "^26.1.0",
    "lint-staged": "^13.2.0",
    "rollup": "^3.19.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.10"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "rxdrag"
  ],
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --ext .ts,.tsx,.js --fix",
      "pretty-quick --staged",
      "git add"
    ],
    "*.md": [
      "pretty-quick --staged",
      "git add"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@rxdrag/shared": "1.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux": "^4.2.1"
  }
}